{
  "backup": {
    "target-repositories": ["/repository/path"],
    "includes": ["/file/path/or/folder/path"],
    "excludes": [
      "**/.git",
      "**/bin",
      "**/obj",
      "**/build",
      "**/*.egg-info",
      "**/__pycache__"
    ],
    "copy-destinations": ["my-server:/path/on/server", "/some/local/path"]
  },
  "custom-commands": {
    "health": [
      {
        "command": "echo works",
        "comment": "health check"
      }
    ]
  }
}
